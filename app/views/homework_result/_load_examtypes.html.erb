<%= select  :homework_result, :homeworktype_id, @homeworktypes.map { |e| [e.name, e.id] },
  {:prompt => "Select homeworktype"},
  {:onchange =>"#{remote_function(:url => {:action => "load_course_all_student"},
  :with => "'homework_type_id='+value+'&course_id='+$('homework_result_course_id').value",
  :before => "Element.show('loader')",
  :success => "Element.hide('loader')"
  )}"}%>
