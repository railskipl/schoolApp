	<div id="masthead">
		
		<div class="content_pad">
			<div id="content-header">
			  <img src="/images/show_timetable.png" />
			  <h1>Timetable</h1>
			  <h3>Select class to edit</h3>
			  <div id="app-back-button">
			    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
			  </div>
			</div>
		</div> <!-- .content_pad -->
		
	</div> <!-- #masthead -->	
	
	<div id="content" class="xgrid">
		<div id="page-yield">
		
		  <% unless flash[:notice].nil? %>
		    <p class="flash-msg"> <%= flash[:notice] %> </p>
		  <% end %>
		
		  <% form_for :timetable_entry do |s| %>
		
		    <div class="label-field-pair">
		      <label for="timetable_entry_course_id">Select a batch</label>
		      <div class="text-input-bg"><%= select :timetable_entry, :batch_id, @batches.map {|c| [c.full_name, c.id]},:prompt=>'Select a batch'  %></div>
		    </div>
		
		    <div class="submit-button">
		      <%= submit_tag "", :value => "â–º Go" %>
		    </div>
		    
		  <% end %>
		
		</div>			
	</div> <!-- #content -->