	<div id="masthead">
		
		<div class="content_pad">
			<div id="content-header">
			  <img src="/images/examination/show_exam.png" alt="Exam" />
			  <h1>Batches</h1>
			  <h3>New Electives</h3>
			  <div id="app-back-button">
			    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
			  </div>
			</div>
		</div> <!-- .content_pad -->
		
	</div> <!-- #masthead -->	
	
	<div id="content" class="xgrid">
		
		<div id="inner-tab-menu">
		  <ul>
		    <li><%= link_to 'New', new_batch_elective_group_path(@batch) %></li>
		
		  </ul>
		</div>
		<div id="page-yield">
		
		  <% unless flash[:notice].nil? %>
		    <p class="flash-msg"> <%= flash[:notice] %> </p>
		  <% end %>
		    
		<div class="bread_crumb">
		        <%= link_to 'Courses', courses_path %> &raquo;
		        <%= link_to @course.full_name, @course %> &raquo;
		        <%= link_to "Batch #{@batch.name}", [@course, @batch] %> &raquo;
		        <%= link_to 'Electives', batch_elective_groups_path(@batch) %> &raquo;
		        New
		        </div>
		  <div class="box">
		
		
		    <% form_for [@batch, @elective_group] do |f| %>
		    <%= f.error_messages :header_message => nil %>
		    
		      <div class="label-field-pair">
		        <div class="label-container"> <%= f.label :name %></div>
		        <div class="input-container"><%= f.text_field :name %></div>
		      </div>
		
		      <%= submit_tag "", :value => "â–º Save", :class => "submit_button" %>
		
		    <% end %>
		
		    <div class="extender"></div>
		  </div>
		</div>
		
	</div> <!-- #content -->