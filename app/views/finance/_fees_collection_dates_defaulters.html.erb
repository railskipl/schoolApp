<%= select  :fees_defaulters, :dates_id, @dates.map { |e| [e.full_name, e.id]},
  {:prompt => "Select fees collection date"},
  {:onChange => "#{remote_function( :url =>
    {:action => "fees_defaulters_students"},
    :with => "'date='+value+'&batch_id='+$('fees_defaulters_batch_id').value") }"} %>