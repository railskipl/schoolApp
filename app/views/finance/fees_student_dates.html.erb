	<div id="masthead">
		
		<div class="content_pad">	
			<div id="content-header">
			    <img src="/images/show_finance.png" alt="" />
			  <h1>Fees Submission</h1>
			  <h3><%= @student.first_name+" "+@student.last_name%></h3>
			  <div id="app-back-button">
			    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
			  </div>
			</div>			
		</div> <!-- .content_pad -->
		
	</div> <!-- #masthead -->	
	
	<div id="content" class="xgrid">
		
		<div id="page-yield">
		  <div class="label-field-pair">
		    <label>Select a Fee Collection date</label>
		  <div class="text-input-bg">
		    <%= select  :fees_submission, :dates_id, @dates.map { |e| [e.full_name, e.id]},
		      {:prompt => "Select fees collection date"},
		      {:onChange => "#{remote_function( :url =>
		      {:action => "fees_submission_student"},
		      :with => "'date='+value+'&id='+#{@student.id}") }"} %>
		  </div>
		    </div>
		      </div>
		<div id= "fee_submission">
		
		</div>
		
		<div id="modal-box" style="display:none;"></div>		
		
	</div> <!-- #content -->