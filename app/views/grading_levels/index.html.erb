    <div id="masthead">
		
		<div class="content_pad">
			<div id="content-header">
			  <img src="/images/examination/show_exam.png" alt="Exam" />
			  <h1>Exams</h1>
			  <h3>Grading levels</h3>
			  <div id="app-back-button">
			      <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
			  </div>
			</div>
		</div> <!-- .content_pad -->
		
	</div> <!-- #masthead -->	
	
	<div id="content" class="xgrid">
		<div id="inner-tab-menu">
		  <ul>
		    <li><%= link_to 'Courses', courses_path %></li>
		  </ul>
		</div>
		<div id="page-yield">
		
		    <% unless flash[:notice].nil? %>
		    <p class="flash-msg"> <%= flash[:notice] %> </p>
		  <% end %>
		
		 <div class="box">
		   <div class="label-field-pair">
		   <label for="student_course">Courses
		<%= image_tag("loader.gif", :align => "absmiddle", :border => 0, :id => "loader", :style =>"display: none;" ) %>
		   </label>
		   <div class="text-input-bg">
		<%= select :batch, :id,
		  @batches.find(:all, :order => 'id ASC').map {|b| [b.full_name, b.id] },
		  {:prompt => 'Common'},
		  {:onchange => "#{remote_function(
		  :url => { :action => 'show' },
		  :with => "'batch_id='+value",
		  :before => "Element.show('loader')",
		  :success => "Element.hide('loader')"  )}"} %>
		   </div></div>
		
		
		<div id="grading-levels" ><%= render :partial => "grading_levels" %></div>
		<div id="modal-box" style="display:none;"></div>
		
		
		</div>
		    </div>		
	</div> <!-- #content -->
